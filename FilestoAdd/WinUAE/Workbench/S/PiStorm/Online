.KEY IPSTACK/K/A,DEVICE/K/A
.BRA {
.KET }

failat 21

If {IPSTACK} EQ "MIAMI"
 assign exists Miami: >NIL:
 if WARN
  echo "Miami not installed!"
  QUIT 10
 endif
endif

If {DEVICE} EQ "WIFIPI"
 execute sys:s/WifiOnline
 if ERROR
 QUIT 10
 endif
endif

If {IPSTACK} EQ "MIAMI"
 if not EXISTS Miami:{DEVICE}.default
  echo "Configuration file Miami:{DEVICE}.default does not exist!"
  Quit 10
 endif
 rx sys:s/MiamiOnline.rexx Miami:{DEVICE}.default
 if ERROR
  QUIT 10
 endif
endif

if not exists sys:s/SetTime
 echo "SetTime script does not exist! Cannot set time!"
 Quit 5
endif
execute sys:s/SetTime

failat 10
Quit 0